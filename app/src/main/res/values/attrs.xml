<?xml version="1.0" encoding="utf-8" ?>
<resources>

    <!--圆形倒计时-->
    <declare-styleable name="CountDownProgress">
        <!-- 圆实心的颜色 -->
        <attr name="circSolidColor" format="color" />
        <!-- 圆边框的颜色 -->
        <attr name="circFrameColor" format="color" />
        <!-- 进度条的颜色 -->
        <attr name="progressColor" format="color" />
        <!-- 文字的颜色 -->
        <attr name="textColor" format="color" />

    </declare-styleable>

    <declare-styleable name="CircularMusicProgressBar">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color|reference" />
        <attr name="border_overlay" format="boolean" />
        <attr name="draw_anticlockwise" format="boolean" />
        <attr name="enable_touch" format="boolean" />
        <attr name="fill_color" format="color|reference" />
        <attr name="progress_color" format="color|reference" />
        <attr name="centercircle_diammterer" format="float|reference" />
        <attr name="progress_startAngle" format="float|reference" />
    </declare-styleable>
    
</resources>